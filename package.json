{
  "name": "probematch",
  "version": "2.0.0",
  "description": "A small module for matching probes to road networks",
  "keywords": [
    "gps",
    "match",
    "probe",
    "geojson"
  ],
  "main": "probematch.js",
  "scripts": {
    "lint": "eslint probematch.js test/*.js",
    "pretest": "npm run lint",
    "test": "tap -Rspec test/test.js"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "strict": [
        0
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "new-cap": [
        0
      ]
    }
  },
  "author": "Tim Channell",
  "license": "ISC",
  "dependencies": {
    "cheap-ruler": "^2.4.0",
    "geojson-flatten": "^0.2.0",
    "geojson-normalize": "0.0.1",
    "rbush": "^1.4.3",
    "turf-linestring": "^1.0.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-mourner": "^1.0.1",
    "turf-point": "^2.0.1",
    "tap": "^5.1.1"
  }
}
